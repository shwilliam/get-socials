// https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id         Int       @default(autoincrement()) @id
  slug       String    @unique
  target     String
  createdAt  DateTime  @default(now())
  author     String
}
